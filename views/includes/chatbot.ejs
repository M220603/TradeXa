<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Chatbot</title>
    <!-- <script src="https://cdn.botpress.cloud/webchat/v2.3/inject.js"></script> -->

    <script src="https://cdn.botpress.cloud/webchat/v2.2/inject.js"></script>

    
</head>
<body>
    <h1>Welcome to Stock Gyanni</h1> <!-- Add some visible content -->
    <div id="webchat" style="width: 500px; height: 500px;"></div>

   <!-- In your <head> tag -->
<!-- <script src="https://cdn.botpress.cloud/webchat/v2.3/inject.js"></> -->
<script src="https://cdn.botpress.cloud/webchat/v2.2/inject.js"></script>
<style>
  #webchat .bpWebchat {
    position: unset;
    width: 100%;
    height: 100%;
    max-height: 100%;
    max-width: 100%;
  }

  #webchat .bpFab {
    display: none;
  }
</style>

<!-- Put this on your page BEFORE the script below -->
<div id="webchat" style="width: 500px; height: 500px;"></div>

<!-- In your <body> tag -->
<script>
  window.botpress.on("webchat:ready", () => {
    window.botpress.open();
  });
  window.botpress.init({
  "botId": "63eccecf-f6be-4371-8b91-9a1853bfd2fb",
  "configuration": {
    "website": {},
    "email": {},
    "phone": {},
    "termsOfService": {},
    "privacyPolicy": {},
    "color": "#2400ff",
    "variant": "solid",
    "themeMode": "light",
    "fontFamily": "inter",
    "radius": 1
  },
  "clientId": "35894db0-0e95-4513-b8e5-3562ec5bd4fa",
  "selector": "#webchat"
});
</script>
</body>
</html>